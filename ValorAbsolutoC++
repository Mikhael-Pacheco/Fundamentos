// Valor Absoluto.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main() {
	int x,a;
	cout<<"Ingresar el valor:"<<endl;
	cin>>a;
	if(a>0) {
		x=a;}
	else{
		x=-a;
	}
	cout<<" El valor absoulto de "<<a<< "es:"<< x;
	getch();
}

